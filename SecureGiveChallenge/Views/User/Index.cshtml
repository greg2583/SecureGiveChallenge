@model IEnumerable<User>
@{
    ViewData["Title"] = "Index";
}

<h2>User List</h2>
<table style="width:50%">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>User Type</th>
        </tr>
    </thead>
    <tbody>
        @foreach (User u in Model)
        {
            <tr>
                <td>@u.FirstName</td>
                <td>@u.LastName</td>
                <td>@u.UserType</td>
            </tr>
        }
    </tbody>
</table>
